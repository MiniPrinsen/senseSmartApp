<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    </style>

    <style name="myrating" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/custom_bar</item>
        <item name="android:minHeight">24dp</item>
        <item name="android:maxHeight">24dp</item>

    </style>
    <style name="CustomTabText" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">12sp</item>
        <item name="android:title">@string/app_name</item>
    </style>
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>
